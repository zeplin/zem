    "@babel/preset-env": "^7.8.4",
    "@zeplin/extension-model": "^1.1.1",
    "adm-zip": "^0.4.11",
    "babel-loader": "^8.0.6",
    "chalk": "^2.3.1",
    "commander": "^2.14.1",
    "eslint": "^4.18.1",
    "eslint-loader": "^2.0.0",
    "fs-extra": "^5.0.0",
    "jsonwebtoken": "^8.3.0",
    "prismjs": "^1.11.0",
    "prompts": "^0.1.14",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "webpack": "^4.41.5",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.1.2"
  },
  "devDependencies": {
    "@zeplin/eslint-config": "^1.2.2"
  },
  "eslintConfig": {
    "extends": "@zeplin/eslint-config/node",
    "rules": {
      "no-process-exit": "off",
      "no-sync": "off"
    }
  },
  "eslintIgnore": [
    "src/template/*"
  ]
}
