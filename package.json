{
  "name": "zem",
  "version": "0.3.4",
  "description": "Create, test and publish Zeplin extensions with no build configuration",
  "homepage": "https://zeplin.io",
  "keywords": [
    "zeplin",
    "zeplin-extension",
    "extension"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zeplin/zem.git"
  },
  "engines": {
    "node": ">=6"
  },
  "bin": {
    "zem": "./src/index.js"
  },
  "files": [
    "src"
